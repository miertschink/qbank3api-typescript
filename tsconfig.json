{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "module": "commonjs",
    "target": "es6",
  },
  "include": [
    "src/**/*.ts",
    "tests/**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
