{
  "extends": [
    "./node_modules/gts/",
    "plugin:@typescript-eslint/recommended-requiring-type-checking"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "ignorePatterns": [
    "build",
    "jest.config.js",
  ],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 100,
        "ignoreTemplateLiterals": true,
        "ignoreUrls": true,
        "ignorePattern": "^import .*"
      }
    ]
  }
}
